{
  "name": "hapi-auth-saml",
  "aliases": [
    "saml-login.no"
  ],
  "type": "npm",
  "env": {
    "NODE_ENV": "production",
    "SAML_DEFAULT_URL": "@saml-default-url",
    "SAML_YAR_SECRET": "@saml-yar-secret",
    "SAML_ENCRYPTOR_SECRET": "@saml-encryptor-secret",
    "SAML_JWT_SECRET": "@saml-jwt-secret",
    "SAML_LOGOUT_URL": "@saml_logout_url",
    "SAML_PRIV_CERT_FILE": "@saml-priv-cert-file",
    "SAML_PRIV_DEV_FILE": "@saml-priv-dev-file",
    "SAML_CERT_FILE": "@saml-cert-file",
    "SAML_LOGOUT_URL": "@saml-logout-url",
    "SAML_LOGIN_PATH": "@saml-login-path",
    "SAML_ENTRY_POINT": "@saml-entry-point",
    "SAML_ISSUER": "@saml-issuer"
  }
}

