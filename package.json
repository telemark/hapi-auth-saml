{
  "name": "hapi-auth-saml",
  "version": "1.0.2",
  "description": "Hapi auth strategy aims to work easily with saml (idporten and feide)",
  "author": "Jonas Enge <jonas.enge@gmail.com> (http://github.com/maccyber)",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm && now alias",
    "start": "node index.js",
    "dev": "NODE_ENV=development node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/hapi-auth-saml.git"
  },
  "devDependencies": {
    "ava": "3.5.0",
    "coveralls": "3.0.9",
    "nyc": "15.0.0",
    "standard": "14.3.1"
  },
  "dependencies": {
    "axios": "0.19.2",
    "boom": "6.0.0",
    "debug": "4.1.1",
    "hapi": "18.1.0",
    "hapi-passport-saml": "1.3.0",
    "jsonwebtoken": "8.5.1",
    "simple-encryptor": "3.0.0",
    "winston": "2.4.4",
    "winston-papertrail": "1.0.5",
    "yar": "9.1.0"
  }
}
