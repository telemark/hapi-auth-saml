{
  "name": "hapi-auth-saml",
  "version": "1.0.0",
  "description": "Hapi auth strategy aims to work easily with saml (idporten and feide)",
  "author": "Jonas Enge <jonas.enge@gmail.com> (http://github.com/maccyber)",
  "license": "MIT",
  "main": "server.js",
  "engines": {
    "node": ">=7.7.3"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm && now alias",
    "start": "node index.js",
    "dev": "NODE_ENV=development node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/idporten.git"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "coveralls": "2.12.0",
    "nsp": "2.6.3",
    "nyc": "10.1.2",
    "standard": "9.0.1"
  },
  "dependencies": {
    "boom": "4.2.0",
    "debug": "2.2.0",
    "good": "^7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "hapi-passport-saml": "0.1.1",
    "jsonwebtoken": "7.3.0",
    "simple-encryptor": "1.1.0",
    "yar": "8.1.2"
  }
}
